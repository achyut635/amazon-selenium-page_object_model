<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Regression Suite" verbose="1" parallel="tests" thread-count="3">
  <listeners>
    <listener class-name="base.AllureTestListener"/>
  </listeners>
  <parameter name="headed" value="false"/>
  <test name="Regression">
    <groups>
      <run>
        <include name="regression"/>
        <exclude name="smoke"/>
      </run>
    </groups>
    <classes>
      <class name="tests.TestCases"/>
      <class name="tests.ProductFlowTest"/>
    </classes>
  </test>
</suite>
